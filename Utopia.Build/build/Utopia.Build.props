<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <!--defining properties interesting for my task-->
    <PropertyGroup>
        <_UtopiaBuild_Directory>$(MSBuildThisFileDirectory)\..\lib\netstandard2.0</_UtopiaBuild_Directory>
        <_UtopiaBuild_Assembly>$(_UtopiaBuild_Directory)\$(MSBuildThisFileName).dll</_UtopiaBuild_Assembly>
    </PropertyGroup>

    <UsingTask TaskName="$(MSBuildThisFileName).RunUtopiaToolTask" AssemblyFile="$(_UtopiaBuild_Assembly)"/>
    <UsingTask TaskName="$(MSBuildThisFileName).GeneratePluginSourceTask" AssemblyFile="$(_UtopiaBuild_Assembly)"/>
    
</Project>
