{
    "metadata": [
        {
            "src": [
                {
                    "files": [ "**/*.csproj" ],
                    "src": "./"
                }
            ],
            "dest": "docfx/api",
            "properties": {
                "TargetFramework": "net6.0"
            }
        }
    ],
    "build": {
        "sitemap": {
            "baseUrl": "https://docs.utopia.mingmoe.moe/",
            "changefreq": "daily"
        },
        "content": [
            {
                "files": "**/*.md",
                "dest": "articles"
            },
            {
                "files": "**/*.yml",
                "dest": "api"
            }
        ],
        "dest": "docfx/site"
    }
}
