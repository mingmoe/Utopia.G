{
    "metadata": [
        {
            "src": [
                {
                    "files": [ "**/*.csproj" ],
                    "src": "./"
                }
            ],
            "dest": "docfx/api"
        }
    ],
    "build": {
        "content": [
            {
                "files": "**/*.md",
                "dest": "articles"
            },
            {
                "files": "**/*.yml",
                "dest": "api"
            }
        ],
        "dest": "docfx/site"
    }
}
